{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "commonjs",
    "resolveJsonModule": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": [
      "dom",
      "dom.iterable",
      "es5",
      "es6",
      "es2015"
    ],
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true
  },
}
