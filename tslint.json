{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [true, "never-prefix"],
        "object-literal-sort-keys": false,
        "arrow-parens": [true, "ban-single-arg-parens"],
        "object-curly-spacing": true,
        "array-bracket-spacing": [
            true,
            "never"
        ]
    },
    "rulesDirectory": []
}
